{
    "name": "bregananta/blockchain",
    "description": "Blockchain for Laravel",
    "keywords": ["blockchain", "laravel"],
    "license": "MIT",
    "authors": [
        {
            "name": "Kunto Bregananta Adi",
            "email": "bregananta@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.0.0",
        "blockchain/blockchain": ">=1.4.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~5.7",
        "orchestra/testbench": "~3.0"
    },
    "autoload": {
       "psr-4": {
            "Bregananta\\Blockchain\\": "src/"
        }
    },
    "minimum-stability": "stable",
    "extra": {
        "laravel": {
            "providers": [
                "Bregananta\\Blockchain\\BlockchainServiceProvider"
            ],
            "aliases": {
                "Blockchain": "Bregananta\\Blockchain\\BlockchainFacade"
            }
        }
    }
}
